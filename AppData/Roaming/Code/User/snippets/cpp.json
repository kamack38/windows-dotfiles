  },

  "fastPowMod": {
    "prefix": "!fastPowMod",
    "body": [
      "long long myPow(int a, int b, int m) {",
      "  if (b == 0) return 1;",
      "  if (b % 2 == 0) {",
      "    long long tmp = myPow(a, b / 2, m);",
      "    return (tmp * tmp) % m;",
      "  } else {",
      "    long long tmp = myPow(a, b / 2, m);",
      "    return (((tmp * tmp) % m) * a) % m;",
      "  }",
      "}"
    ],
    "description": "Quickly calculate a^b mod c"
  },

  "longlong": {
    "prefix": "!LL",
    "body": ["using LL = long long"],
    "description": "Create lias LL of long long"
