{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": "closePane",
            "keys": "ctrl+w"
        },
        {
            "command": "find",
            "keys": "ctrl+f"
        },
        {
            "command": {
                "action": "quakeMode"
            },
            "keys": "alt+q"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+w"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+t"
        },
        {
            "command": "unbound",
            "keys": "alt+shift+s"
        },
        {
            "command": "unbound",
            "keys": "alt+shift+c"
        },
        {
            "command": "toggleAlwaysOnTop",
            "keys": "alt+t"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "name": "New tab",
            "command": {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "name": "Delete previous word",
            "command": {
                "action": "sendInput",
                "input": "\u0017"
            },
            "keys": "ctrl+backspace"
        },
        {
            "name": "Delete next word",
            "command": {
                "action": "sendInput",
                "input": "\u001B\u0064"
            },
            "keys": "ctrl+delete"
        },
        {
            "command": {
                "action": "sendInput",
                "input": "cls\r"
            },
            "keys": "alt+c"
        },
        {
            "command": "openNewTabDropdown",
            "keys": "alt+n"
        },
        {
            "command": "toggleFocusMode",
            "keys": "alt+shift+f"
        },
        {
            "command": {
                "action": "sendInput",
                "input": "cd ..\r"
            },
            "keys": "ctrl+up"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": {
                "action": "splitPane"
            }
        },
        {
            "command": "togglePaneZoom",
            "keys": "alt+z"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
    "experimental.rendering.software": false,
    "initialCols": 126,
    "initialRows": 34,
